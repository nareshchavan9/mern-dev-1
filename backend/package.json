    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "nodemon": "^2.0.15",
